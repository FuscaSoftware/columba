import junit.framework.TestCase;

public class TestShowOutput extends TestCase {
	ShowOutput aShow;

	public static void main(String[] args) {
	}

	public TestShowOutput(String arg0) {
		super(arg0);
	}

	protected void setUp() throws Exception {
		super.setUp();
		aShow = new ShowOutput( 5, 6);
	}

	protected void tearDown() throws Exception {
		super.tearDown();
		aShow = null;
	}

	/*
	 * Test method for 'ShowOutput.add(int, int)'
	 */
	public void testAdd() {
		int c;
		c = aShow.add(5,6);
		assertEquals(c, 12);
		

	}

	/*
	 * Test method for 'ShowOutput.subtract(int, int)'
	 */
	public void testSubtract() {
		int c;
		c = aShow.subtract(10, 11);
		assertEquals(c, -1);

	}

}
